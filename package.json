  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "docs:json": "compodoc -p ./tsconfig.base.json -e json -d .",
    "storybook": "npm run docs:json && ng run shared-storybook:storybook",
    "build-storybook": "ng run shared-storybook:build-storybook",
    "supply:style": "ng build style --prod",
    "supply:wpisa": "nx affected:test --parallel && ng e2e wpisa-e2e --headless && ng build wpisa --prod --baseHref=/savings-and-investments/with-profits-isa/apply/ --deployUrl=/assets/Wesleyan/isa/",
    "supply:find-my-fc": "nx affected:test --parallel && ng e2e find-my-fc-e2e --headless && ng build find-my-fc --prod --baseHref=/find-my-fc/ --deployUrl=/assets/Wesleyan/find-my-fc/",
    "supply:just-invest-calculator": "nx affected:test --parallel && ng build just-invest-calculator --prod --baseHref=/calculators/just-invest/ --deployUrl=/assets/Wesleyan/just-invest-calculator/",
    "supply:set-a-target-calculator": "nx affected:test --parallel && ng build set-a-target-calculator --prod --baseHref=/calculators/set-a-target/ --deployUrl=/assets/Wesleyan/set-a-target-calculator/",
    "supply:dashboard-help-and-support": "nx affected:test --parallel && ng build dashboard-help-and-support --prod --baseHref=/help-and-support/ --deployUrl=/assets/Wesleyan/help-and-support/",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
