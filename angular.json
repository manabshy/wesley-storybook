{
  "version": 1,
  "projects": {
    "wpisa": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/wpisa",
      "sourceRoot": "apps/wpisa/src",
      "prefix": "wes",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/wpisa",
            "index": "apps/wpisa/src/index.html",
            "main": "apps/wpisa/src/main.ts",
            "polyfills": "apps/wpisa/src/polyfills.ts",
            "tsConfig": "apps/wpisa/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/wpisa/src/favicon.ico", "apps/wpisa/src/assets"],
            "styles": ["libs/shared/ui-styles/src/index.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/ui-styles/src/lib"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wpisa/src/environments/environment.ts",
                  "with": "apps/wpisa/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wpisa:build",
            "proxyConfig": "apps/wpisa/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "wpisa:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wpisa:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/wpisa/tsconfig.app.json",
              "apps/wpisa/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/wpisa/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/wpisa/jest.config.js",
            "tsConfig": "apps/wpisa/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/wpisa/src/test-setup.ts"
          }
        }
      }
    },
    "wpisa-e2e": {
      "root": "apps/wpisa-e2e",
      "sourceRoot": "apps/wpisa-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wpisa-e2e/cypress.json",
            "tsConfig": "apps/wpisa-e2e/tsconfig.e2e.json",
            "devServerTarget": "wpisa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wpisa:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/wpisa-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/wpisa-e2e/**"]
          }
        }
      }
    },
