    "prefix": "wes"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "style": "scss"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "prefix": "wes"
    }
  },
  "defaultProject": "wpisa"
